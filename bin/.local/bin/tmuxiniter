#!/usr/bin/env bash
tmux_running=$(pgrep tmux)

if [[ -z $TMUX ]] && [[ -z $tmux_running ]]; then
    tmux new -s init txs
else  
    selected=$(tmux list-session | fzf | awk '{print $1}' | tr -d ':')
    tmux attach -t $selected

    if [[ -z $selected ]]; then
        txs
        # exit 0
    fi
fi

