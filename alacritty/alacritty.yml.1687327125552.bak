window:
  decorations: none
  startup_mode: Maximized
  option_as_alt: Both
  padding:
    x: 4
    y: 4
font:
  normal:
    family: Liga SFMono Nerd Font
    # family: Liga SFMono Nerd Font
    style: Regular

  bold:
    family: Liga SFMono Nerd Font
    # family: Liga SFMono Nerd Font
    style: Bold

  italic:
    family: Liga SFMono Nerd Font
    # family: Liga SFMono Nerd Font
    style: Italic

  bold_italic:
    family: Liga SFMono Nerd Font
    #0 family: Liga SFMono Nerd Font
    style: Bold Italic
  size: 18
shell:
  program: /usr/local/bin/fish
  args:
    - -l
    - -c
    - "/usr/local/bin/tmuxiniter"
colors:
  name: Greenscreen (dark)
  author: Chris Kempson
  primary:
    background: "#001100"
    foreground: "#00bb00"
  cursor:
    text: "#001100"
    cursor: "#00bb00"
  normal:
    black: "#001100"
    red: "#007700"
    green: "#00bb00"
    yellow: "#007700"
    blue: "#009900"
    magenta: "#00bb00"
    cyan: "#005500"
    white: "#00bb00"
  bright:
    black: "#007700"
    red: "#007700"
    green: "#00bb00"
    yellow: "#007700"
    blue: "#009900"
    magenta: "#00bb00"
    cyan: "#005500"
    white: "#00ff00"
theme: Greenscreen.dark
