window:
  decorations: none
  startup_mode: Maximized
  option_as_alt: Both
  padding:
    x: 4
    y: 4
font:
  normal:
    family: MesloLGS Nerd Font
    # family: MesloLGS Nerd Font
    style: Regular

  bold:
    family: MesloLGS Nerd Font
    # family: MesloLGS Nerd Font
    style: Bold

  italic:
    family: MesloLGS Nerd Font
    # family: MesloLGS Nerd Font
    style: Italic

  bold_italic:
    family: MesloLGS Nerd Font
    # family: MesloLGS Nerd Font
    style: Bold Italic
  size: 13
shell:
  program: /usr/local/bin/fish
  args:
    - -l
    - -c
    - "/usr/local/bin/tmuxiniter"
colors:
  name: 3024 (dark)
  author: Chris Kempson
  primary:
    background: "#090300"
    foreground: "#a5a2a2"
  cursor:
    text: "#090300"
    cursor: "#a5a2a2"
  normal:
    black: "#090300"
    red: "#db2d20"
    green: "#01a252"
    yellow: "#fded02"
    blue: "#01a0e4"
    magenta: "#a16a94"
    cyan: "#b5e4f4"
    white: "#a5a2a2"
  bright:
    black: "#5c5855"
    red: "#db2d20"
    green: "#01a252"
    yellow: "#fded02"
    blue: "#01a0e4"
    magenta: "#a16a94"
    cyan: "#b5e4f4"
    white: "#f7f7f7"
theme: 3024.dark
