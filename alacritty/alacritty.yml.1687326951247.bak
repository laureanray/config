window:
  decorations: none
  startup_mode: Maximized
  option_as_alt: Both
  padding:
    x: 4
    y: 4
font:
  normal:
    family: Liga SFMono Nerd Font
    # family: Liga SFMono Nerd Font
    style: Regular

  bold:
    family: Liga SFMono Nerd Font
    # family: Liga SFMono Nerd Font
    style: Bold

  italic:
    family: Liga SFMono Nerd Font
    # family: Liga SFMono Nerd Font
    style: Italic

  bold_italic:
    family: Liga SFMono Nerd Font
    #0 family: Liga SFMono Nerd Font
    style: Bold Italic
  size: 18
shell:
  program: /usr/local/bin/fish
  args:
    - -l
    - -c
    - "/usr/local/bin/tmuxiniter"
colors:
  # Default Colors
  primary:
    background: '#212121'
    foreground: '#f1f1f1'
  # Normal colors
  normal:
    black: '#212121'
    red: '#c30771'
    green: '#10a778'
    yellow: '#a89c14'
    blue: '#008ec4'
    magenta: '#523c79'
    cyan: '#20a5ba'
    white: '#e0e0e0'
  # Bright colors
  bright:
    black: '#212121'
    red: '#fb007a'
    green: '#5fd7af'
    yellow: '#f3e430'
    blue: '#20bbfc'
    magenta: '#6855de'
    cyan: '#4fb8cc'
    white: '#f1f1f1'
theme: Pencil-Dark
ite: "#a59058"
theme: Parker_brothers
