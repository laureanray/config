window:
  padding:
    x: 4
    y: 4
font:
  normal:
    family: JetBrainsMono Nerd Font
    style: Regular

  bold:
    family: JetBrainsMono Nerd Font
    style: Bold

  italic:
    family: JetBrainsMono Nerd Font
    style: Italic

  bold_italic:
    family: JetBrainsMono Nerd Font
    style: Bold Italic

  size: 15
shell:
  program: /usr/local/bin/fish
  args:
    - -l
    - -c
    - "tmux attach || tmux"
colors:
  name: Codeschool (dark)
  author: Chris Kempson
  primary:
    background: "#232c31"
    foreground: "#9ea7a6"
  cursor:
    text: "#232c31"
    cursor: "#9ea7a6"
  normal:
    black: "#232c31"
    red: "#2a5491"
    green: "#237986"
    yellow: "#a03b1e"
    blue: "#484d79"
    magenta: "#c59820"
    cyan: "#b02f30"
    white: "#9ea7a6"
  bright:
    black: "#3f4944"
    red: "#2a5491"
    green: "#237986"
    yellow: "#a03b1e"
    blue: "#484d79"
    magenta: "#c59820"
    cyan: "#b02f30"
    white: "#b5d8f6"
theme: Codeschool.dark
